    <script type="text/javascript">
        $(document).ready(function () {
            $("#btnGrabar").click(function () {
                var firstDate = $("#txtFechaInicio").val();
                var secondDate = $("#txtFechaFin").val();
                var facturado = $("#txtEstadoFacturacion").val();
                if (facturado == 'P') {
                    alert("La cotización ya fue facturada ,no se puede modificar");
                }

                if (firstDate > secondDate) {
                    alert("La fecha de Inicio debe ser menor a la fecha Fin de Vigencia ");
                }

            });

            $.validator.addMethod("enddate", function (value, element) {
                var startdatevalue = $('.startdate').val();
                return Date.parse(startdatevalue) < Date.parse(value);
            });
            $('#frmCotizacion').validate({
                rules: {
                    EmpresaRUC: {
                        required: true, minlength: 11
                    },
                    EmpresaNombre: {
                        required: true
                    },
                    Direccion: {
                        required: true
                    },
                    CodigoDpto: {
                        required: true
                    },
                    CodigoProv: {
                        required: true
                    },
                    CodigoDist: {
                        required: true
                    },
                    CodigoDptoR: {
                        required: true
                    },
                    CodigoProvR: {
                        required: true
                    },
                    CodigoDistR: {
                        required: true
                    },
                    dtm_FechaInicio: {
                        required: true
                    },
                    dtm_FechaFin: {
                        required: true,
                        enddate: "dtm_FechaInicio"
                    },
                    EstadoFacturacion: {
                        equalTo: 'P'
                    },
                },
                messages: {
                    EmpresaRUC: "Ingrese un  RUC",
                    EmpresaNombre: "Ingrese una Razón Social",
                    Direccion: "Ingrese una Dirección",
                    CodigoDpto: "Seleccione un Departamento",
                    CodigoProv: "Seleccione una Provincia",
                    CodigoDist: "Seleccione un Distrito",
                    CodigoDptoR: "Seleccione un Departamento para el riesgo",
                    CodigoProvR: "Seleccione una Provincia para el riesgo",
                    CodigoDistR: "Seleccione un Distrito para el riesgo",
                    //EstadoFacturacion : "Cotización ya está facturada",
                    //dtm_FechaInicio: "Ingrese fecha de Inicio",
                    //dtm_FechaFin: { required :"Ingrese fecha de Fin",enddate :"Fecha Fin deber ser mayor a Fecha de Inicio"},

                    dtm_FechaInicio: {
                        required: "Ingrese una fecha de Inicio"
                    },
                    dtm_FechaFin: {
                        required: "Ingrese una fecha de Inicio",
                        enddate: "Fecha Fin debe ser mayor a Fecha de Inicio !"
                    },
                    EstadoFacturacion: {
                        equalTo: "Cotización ya está facturada"
                    },

                },
                errorElement: 'span',
                errorPlacement: function (error, element) {
                    error.addClass('invalid-feedback');
                    element.closest('.form-group').append(error);
                },
                highlight: function (element, errorClass, validClass) {
                    $(element).addClass('is-invalid');
                },
                unhighlight: function (element, errorClass, validClass) {
                    $(element).removeClass('is-invalid');
                }
            });
        });
    </script>
