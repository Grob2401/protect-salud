                //alert("Handler for .click() Cotizar called.");
                var empresaruc = $('#txtEmpresaRUC').val();
                var empresanombre = $('#txtEmpresaNombre').val();
                var codigocorredor = '';
                var codigociiu = $('#drpActividadE').val();
                var montooperativos = $('#txtDetMontoPlanillaOpe').val();
                var numerooperativos = $('#txtDetNumeroTrabajadoresOpe').val();
                var montoadministrativos = $('#txtDetMontoPlanillaAdm').val();
                var numeroadministrativos = $('#txtDetNumeroTrabajadoresAdm').val();
                var codigomoneda = $('#drpMoneda').val();
                var fechainicio = $('#txtFechaInicio').val();
                var fechafin = $('#txtFechaFin').val();
                var tiempocobertura = 0;
                var porcentajetasa = $('#txtPorcentajeTasa').val();
                var porcentajecorredor = $('#txtPorcentajeCorredor').val();
                var numerocotizacion = '0';
                var usuario = '';
                var ubigeoriesgo = $('#drpDepartamentoR').val() + $('#drpProvinciaR').val() + $('#drpDistritoR').val();
                var codigodpto = $('#drpDepartamento').val();
                var codigoprov = $('#drpProvincia').val();
                var codigodist = $('#drpDistrito').val();
                var codigodptor = $('#drpDepartamentoR').val();
                var codigoprovr = $('#drpProvinciaR').val();
                var codigodistr = $('#drpDistritoR').val();
                var grupociiu = $('#drpActividadP').val();
                e.preventDefault();
                //alert("Handler for .click() Cotizar called.");
                $.ajax({
                    type: "post",
                    url: url_method,
                    data: { empresaruc, empresanombre, codigocorredor, codigociiu, montooperativos, numerooperativos, montoadministrativos, numeroadministrativos, codigomoneda, fechainicio, fechafin, tiempocobertura, porcentajetasa, porcentajecorredor, numerocotizacion, usuario, ubigeoriesgo, codigodpto, codigoprov, codigodist, codigodptor, codigoprovr, codigodistr, grupociiu },
                    datatype: "json",
                    traditional: true,
                    success: function (response) {
                        if (response != null) {
                            //alert("dentro de Ajax");
                            //alert(response.ImportePrimaNeta);
                            $('#txtImportePrimaNeta').val(response.ImportePrimaNeta);
                            $('#txtImporteIGV').val(response.ImporteIGV);
